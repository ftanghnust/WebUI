@{
    ViewBag.Title = "PriceSales";
    Layout = "~/Views/Shared/_LayoutNew.cshtml";
}

<fieldset style="margin:0px 0px 5px 0px; border: 1px solid #95B8E7">
    <legend style="color:#416AB7; font-weight:bold">信息查询</legend>
    <table id="tab" class="xstable" border="0">
        <tr>
            <td >活动名称：</td>
            <th><input type="text" id="VendorCode" name="VendorCode"></th>
            <td >单据状态：</td>
            <th><input type="text" id="VendorName" name="VendorName"></th>
            <td>录单时间：</td>
            <th>
                <input type="text" id="VendorName1" name="VendorName1">
            </th>
        </tr>
        <tr>
            <td>开始时间：</td>
            <th>
                <input onfocus="WdatePicker({dateFmt:'yyyy-M-d'})" class="Wdate" name="StartDate" id="StartDate" />
            </th>
            <th>结束时间：</th>
            <th>
                <input onfocus="WdatePicker({dateFmt:'yyyy-M-d',minDate:'#F{$dp.$D(\'StartDate\')}'})" class="Wdate" name="EndDate" id="EndDate"/>
            </th>
            
            <th colspan="2">
                <input type="button" id="aSearch" class="aSearch" value="查询"><input  id="aReset" type="reset" class="reset"  value="重置"></th>
        </tr>
    </table>
</fieldset>

<div id="grid"></div>
@section scripts
{
    <script src="@Url.Content("~/Scripts/bussiness/product/priceSales.js")" type="text/javascript"></script>
}