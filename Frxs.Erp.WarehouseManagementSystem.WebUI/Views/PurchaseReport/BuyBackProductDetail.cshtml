@{
    //(采购入库采购退货明细) 
    ViewBag.Title = "芙蓉兴盛ERP管理系统";
    Layout = "~/Views/Shared/_LayoutNew.cshtml";
}
<form id="myForm">
    <fieldset style="margin: 0px 0px 5px 0px; border: 1px solid #95B8E7">
        <legend style="color: #416AB7; font-weight: bold">报表查询</legend>
        <table id="tab" class="xstable" border="0">
            <tr>
                <td>录单开始时间：</td>
                <th>
                    <input onclick="WdatePicker({ dateFmt: 'yyyy-MM-dd HH:mm', maxDate: '#F{$dp.$D(\'S2\')}' })" name="S1" id="S1" class="Wdate easyui-validatebox" required="true" />
                </th>
                <td>录单截止时间：</td>
                <th>
                    <input onclick="WdatePicker({ dateFmt: 'yyyy-MM-dd HH:mm', minDate: '#F{$dp.$D(\'S1\')}' })" name="S2" id="S2" class="Wdate easyui-validatebox" required="true" style="width: 175px" />
                </th>
                <td>采购员：
                </td>
                <th>
                    <input type="text" name="BuyEmpName" id="BuyEmpName" style="width: 145px;" readonly="readonly" /><input type="button" value="..." onclick="selBuyEmp()" />
                    <input type="hidden" name="BuyEmpID" id="S7" />
                </th>
            </tr>
            <tr>
                <td>过账开始时间：</td>
                <th>
                    <input onclick="WdatePicker({ dateFmt: 'yyyy-MM-dd HH:mm', maxDate: '#F{$dp.$D(\'S6\')}' })" name="S5" id="S5" class="Wdate" />
                </th>
                <td>过账截止时间：</td>
                <th>
                    <input onclick="WdatePicker({ dateFmt: 'yyyy-MM-dd HH:mm', minDate: '#F{$dp.$D(\'S5\')}' })" name="S6" id="S6" class="Wdate" style="width: 175px" />
                </th>

                <td>供应商：</td>
                <th colspan="3">
                    <input type="hidden" name="VendorID" id="S3" />
                    <input type="text" name="VendorCode" id="VendorCode" style="width: 100px;"><input type="button" value="..." onclick="selVendor()" />
                    <input type="text" name="VendorName" id="VendorName" style="width: 220px" readonly="readonly" />
                </th>
            </tr>
            <tr>
                <td>过账日：</td>
                <th>
                    <input onclick="WdatePicker({ dateFmt: 'yyyy-MM-dd', maxDate: '#F{$dp.$D(\'S11\')}' })" name="S10" id="S10" class="Wdate" />
                </th>
                <td style="text-align: center">至</td>
                <th>
                    <input onclick="WdatePicker({ dateFmt: 'yyyy-MM-dd', minDate: '#F{$dp.$D(\'S10\')}' })" name="S11" id="S11" class="Wdate" />
                </th>
                <td>仓库：</td>
                <th colspan="2">
                    <select id="SubWID" name="SubWID" class="easyui-combobox" style="width: 175px" panelheight="auto" editable="false"></select>
                </th>
            </tr>
            <tr>
                <td>商品名称：</td>
                <th>
                    <input type="text" name="ProductName" id="S8">
                </th>
                <td>商品编码：</td>
                <th>
                    <input type="text" name="SKU" id="S9">
                </th>
                
                <th colspan="2">
                    <input type="button" id="aSearch" class="aSearch" onclick="search()" value="查询">
                    <input type="button" onclick="resetSearch()" class="reset" value="重置">
                </th>
            </tr>
        </table>
    </fieldset>
</form>
<div id="grid"></div>
@section scripts
{
    <script src="@Url.Content("~/Scripts/plugin/uploadify/FileSaver.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/bussiness/report/buybackproductdetail.js")" type="text/javascript"></script>
}