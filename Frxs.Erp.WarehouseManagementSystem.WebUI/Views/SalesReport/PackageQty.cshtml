@using Frxs.Erp.WarehouseManagementSystem.WebUI
@{
    ViewBag.Title = "周转箱报表";
    Layout = "~/Views/Shared/_LayoutNew.cshtml";
}
<form id="myForm">
    <fieldset style="margin: 0px 0px 5px 0px; border: 1px solid #95B8E7">
        <legend style="color: #416AB7; font-weight: bold">报表查询</legend>
        <table id="tab" class="xstable" border="0">
            <tr>
                <td>开始日期：</td>
                <th>
                    <input onclick="WdatePicker({ dateFmt: 'yyyy-MM-dd', maxDate: '#F{$dp.$D(\'ShippingEndDate\')}' })" name="ShippingBeginDate" id="ShippingBeginDate" class="Wdate" />
                </th>
                <td>结束日期：</td>
                <th colspan="2">
                    <input onclick="WdatePicker({ dateFmt: 'yyyy-MM-dd', minDate: '#F{$dp.$D(\'ShippingBeginDate\')}' })" name="ShippingEndDate" id="ShippingEndDate" class="Wdate" />
                </th>
            </tr>
            <tr>
             <td>司机名称：</td>
                <th>
                    <input type="text" name="ShippingUserName" id="ShippingUserName" />
                </th>
                <td>仓库：</td>
                <th>
                    <select id="WID" name="WID" class="easyui-combobox" style="width: 148px" panelheight="auto" editable="false">
                        <option value="@WorkContext.CurrentWarehouse.WarehouseId">@WorkContext.CurrentWarehouse.WarehouseName</option>
                    </select>
                </th>
                <th>
                    <input type="button" id="aSearch" class="aSearch" onclick="search()" value="查询" />
                    <input type="button" onclick="resetSearch()" class="reset" value="重置" />
                </th>
            </tr>
        </table>
    </fieldset>
</form>
<div id="grid"></div>
@section scripts
{
    <script src="@Url.Content("~/Scripts/plugin/uploadify/FileSaver.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/bussiness/report/packageqty.js")" type="text/javascript"></script>
}