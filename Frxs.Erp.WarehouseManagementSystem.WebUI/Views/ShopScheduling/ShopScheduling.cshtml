@{
    ViewBag.Title = "ShopScheduling";
    Layout = "~/Views/Shared/_LayoutNew.cshtml";
}

<fieldset style="margin: 0px 0px 5px 0px; border: 1px solid #95B8E7">
    <legend style="color: #416AB7; font-weight: bold">信息查询</legend>
    <table id="tab" class="xstable">
        <tr>
            <td>预计配送日期：</td>
            <th>
                <input type="text" id="SearchDate" name="SearchDate" readonly="readonly"></th>
                 <td>配送线路：</td>
            <th>
                <select id="LineId" name="LineId" class="easyui-combobox " style="width: 145px" data-options="panelHeight:'auto',editable:false"  >
                    </select></th>
            <td>门店类型：</td>
           <th>
                <select id="ShopType" name="ShopType" class="easyui-combobox " style="width: 145px" data-options="panelHeight:'auto',editable:false"  >
                    <option value="">-全部-</option>
                    <option value="0">加盟店</option>
                    <option value="1">签约店</option>
                    </select></th>
             <td>是否下单：</td>
           <th>
                <select id="IsOrder" name="IsOrder" class="easyui-combobox " style="width: 145px" data-options="panelHeight:'auto',editable:false"  >
                    <option value="">-全部-</option>
                    <option value="1">已下单</option>
                    <option value="0">未下单</option>
                    </select></th>
             <td>
                <input type="submit" onclick="search(); return false;" id="aSearch"  class="aSearch" value="查询"><input type="button" class="reset" value="重置" onclick="    resetSearch()"></td>
        </tr>
       
    </table>
</fieldset>

<div id="grid"></div>

@section scripts
{ 
    <script src="@Url.Content("~/Scripts/bussiness/shopScheduling/shopSchedulingListNew.js")" type="text/javascript"></script>
}