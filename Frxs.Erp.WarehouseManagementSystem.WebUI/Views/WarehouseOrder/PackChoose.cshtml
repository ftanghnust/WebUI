@{
    ViewBag.Title = "PackChoose";
    Layout = "~/Views/Shared/_LayoutNew.cshtml";
}
<style>
    .xstable .aSearch { font-size:18px; padding-left:30px; padding-right:30px }
    .xstable .reset { font-size:18px;padding-left:30px; padding-right:30px  }
</style>
<center>
<table id="tab" class="xstable" style="width:240px; padding-top:40px">
    <tr>
        <th>
            <input type="button" value="对货" id="btCheck" onclick="packCheck()" class="aSearch"/></th>
        <th>
            <input type="button" value="装箱" id="btPack" onclick="packStart()" class="reset"/></th>
    </tr>
</table>
</center>


@section scripts
{
    <script src="@Url.Content("~/Scripts/bussiness/warehouseOrder/packChoose.js")" type="text/javascript"></script>
}
