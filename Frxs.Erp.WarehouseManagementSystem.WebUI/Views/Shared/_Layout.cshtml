<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Style/css")
</head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<body class="easyui-layout" style="width: 100%; height: 100%; margin: 0px; padding: 0px">
    <div data-options="region:'north'" style="height: 30px; background: #FFFFFF">
        <div class="heardertop">
            <h1>
                <img src="@Url.Content("~/Style/images/logo.jpg")"></h1>
            <h2><a href="#">退出</a></h2>
        </div>
    </div>
    <div data-options="region:'south'" style="height: 27px; background: #F8F8F8">
        <div class="footer">
            <span>公司信息：芙蓉兴盛</span><span>操作员：@this.UserIdentity.UserName  </span>@*<span>仓库：@this.CurrentWarehouse.Parent.WarehouseName-@this.CurrentWarehouse.WarehouseName
            </span>*@
        </div>
    </div>
    <div data-options="region:'west',split:true" title="菜单" style="width: 180px;" class="west">
        <div class="treeDiv">
            <ul class="easyui-tree" id="tree" data-options="method:'get',animate:true,lines:true">
                <li>
                    <span>采购管理管理</span>
                    <ul>
                        <li data-options="state:'closed'">
                            <span>销售</span>
                            <ul>
                                <li>
                                    <span>货位管理</span>
                                </li>
                            </ul>
                        </li>
                    </ul>
                </li>
            </ul>
        </div>
    </div>
    <div data-options="region:'center',iconCls:'icon-ok'" id="mainPanle">
        <div id="tabs" class="easyui-tabs" style="height: 100%">
            <div title="首页">
                <div style="height: 100%">
                    @*<iframe src="add.html" style="width:100%; height:100%; border:none;" frameborder="0" scrolling="no" ></iframe>*@
                    @RenderBody()
                </div>
            </div>
        </div>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/customer")
    @RenderSection("scripts", required: false)
</body>
</html>
