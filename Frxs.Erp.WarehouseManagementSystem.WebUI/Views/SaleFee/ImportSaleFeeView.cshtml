@{
    ViewBag.Title = "ImportSaleFeeView";
    Layout = "~/Views/Shared/_LayoutNew.cshtml";
}
<style type="text/css">
    .textstyle1
    {
        color: Red;
        font-weight: bold;
    }

    .textstyle2
    {
        color: Green;
        font-weight: bold;
    }
      .uploadifyQueue{ float: left;}
    object {
        margin-top: 20px;float: left;margin-right: 10px;
    }
    #tab th,#tab td{ padding: 0px;margin: 0px;}
</style>

<fieldset style="margin: 0px 0px 5px 0px; border: 1px solid #95B8E7">
    <legend style="color: #416AB7; font-weight: bold">导入门店费用</legend>
    <table id="tab" class="xstable">
        <tr>
             <th style="padding-top: 15px;">
                <label for="Attachment_GUID">附件上传：</label>
            </th>
            <td>
                <div>
                    <input type="hidden" id="SaleFee_GUID" name="SaleFee_GUID" />
                    <input id="file_upload" name="file_upload" type="file" >
                </div>
            </td>
        </tr>
        <tr>
            <th>操作：</th>
            <td style="padding-top: 8px;text-align: left;">
                <p style="margin-top: 5px; font-size: 14px; font-weight: bold;">
                    <a href="javascript:if(checkImport()){$('#file_upload').uploadifySettings('scriptData',{'guid':$('#SaleFee_GUID').val()});$('#file_upload').uploadifyUpload();}" class="easyui-linkbutton" iconcls="icon-save"  id="btnUpload"  style="margin-right: 18px">上传文件</a>  <a href="~/FileUpload/门店费用导入模板.xls" target="_blank">下载模板</a> &nbsp; &nbsp; &nbsp; &nbsp;<span id="result"></span>
                </p>
            </td>
        </tr>

    </table>
</fieldset>
<div id="importGrid"></div>
@section scripts
{
    <link href="@Url.Content("~/Scripts/plugin/uploadify/uploadify.css")" rel="stylesheet" />
    <script src="@Url.Content("~/Scripts/plugin/uploadify/jquery.uploadify.v2.1.0.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/plugin/uploadify/swfobject.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/bussiness/saleFee/ImportData.js")"  type="text/javascript"></script>

}
