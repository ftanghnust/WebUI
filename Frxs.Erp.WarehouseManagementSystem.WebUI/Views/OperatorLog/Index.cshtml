@{
    ViewBag.Title = "芙蓉兴盛ERP管理系统";
    Layout = "~/Views/Shared/_LayoutNew.cshtml";
}
<form id="searchform">
    <fieldset style="margin: 0px 0px 5px 0px; border: 1px solid #95B8E7">
        <legend style="color: #416AB7; font-weight: bold">信息查询</legend>
        <table id="tab" class="xstable">
            <tr>
                <td>用户名：</td>
                <th>
                    <input type="text" name="OperatorName" id="OperatorName">
                </th>
                <td>操作时间：</td>
                <th colspan="3">
                    <input onfocus="WdatePicker({dateFmt:'yyyy-M-d HH:mm',maxDate:'#F{$dp.$D(\'EndTime\')}'})" class="Wdate easyui-validatebox" name="BeginTime" id="BeginTime"/>至<input onfocus="WdatePicker({dateFmt:'yyyy-M-d HH:mm',minDate:'#F{$dp.$D(\'BeginTime\')}'})" class="Wdate easyui-validatebox" name="EndTime" id="EndTime" />
                </th>
                <td>模块：</td>
                <th>
                    <select id="MenuID" name="MenuID" class="easyui-combobox" style="width: 200px" data-options="panelHeight:'300',editable:false">
                        
                    </select>
                </th>
                <th>
                    <input type="submit" id="aSearch" class="aSearch" value="查询">
                    <input type="button" id="aReset" class="reset" value="重置">
                </th>
            </tr>
        </table>
    </fieldset>
</form>
<div id="grid"></div>
@section scripts
{
    <script src="@Url.Content("~/Scripts/bussiness/operatorLog/operatorLogList.js")" type="text/javascript"></script>
}